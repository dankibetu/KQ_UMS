<ViewObject
   Name="ReplacementLineVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="xxkq.oracle.apps.xbol.v2.employee.server.ReplacementLineVORowImpl"
   ComponentClass="xxkq.oracle.apps.xbol.v2.employee.server.ReplacementLineVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT rle.requisition_line_id
     , xurh.requisition_header_id
     , rle.uniform_set_line_id
     , ROWNUM
     , rle.inventory_item_id
     , rle.need_by_date
     , rle.remarks
     , mc.segment1 || '.' || mc.segment2 || '.' || mc.segment3 AS item_category
     , xuel.entitled_qty
     , xuel.category_id
     , xueh.uniform_set_id                                     AS uniformsetid
     , xuel.renewel_qty
     , xuel.life_expectancy
     , 100                                                     AS eligible_renewal_qty
     ,       --NVL(XX_UMS_ELIGIBLE_QTY_F(XURH.person_id,XUEL.life_expectancy,XUEL.entitled_qty),0) as eligible_renewal_qty,
      rle.requested_qty
     , rle.damage_qty
     , rle.chargable_qty
     , rle.returned_qty
     , rle.reason_code
     , rle.return_item_id
     , xuel.inventory_organization_id                          AS organization_id
     , SUM(xueo.onhand_qty)
     , msi.description                                         itemdescription
     , xuel.uniform_limited_qty                                AS uniformlimitedquantity
     , msi.primary_unit_of_measure                             uom
     , msi.segment1                                            AS itemcode
  FROM xx_ums_requisition_line_v2   rle
     , xx_ums_requisition_header_v2 xurh
     , xx_ums_entitlement_header_v2 xueh
     , xx_ums_entitlement_line_v2   xuel
     , mtl_categories               mc
     , xx_ums_emp_onhand_v2         xueo
     , mtl_system_items_b           msi
     , fnd_id_flex_structures_vl    ffsv
 WHERE mc.disable_date IS NULL
   AND mc.enabled_flag            = 'Y'
   AND xueh.uniform_set_id        = xuel.uniform_set_id
--AND xuel.enable_flag='Y' 
   AND xuel.category_id           = mc.category_id
--and RLE.UNIFORM_SET_LINE_ID = xuel.UNIFORM_SET_LINE_ID
   AND xurh.requisition_header_id = rle.requisition_header_id
   AND xueh.uniform_set_id        = xueo.uniform_set_id
   AND xuel.uniform_set_line_id   = xueo.uniform_set_line_id
--AND   xurh.person_id=xueo.person_id
   AND xurh.business_group_id     = xueo.business_group_id
   AND msi.inventory_item_id      = rle.inventory_item_id
   AND msi.organization_id        = rle.organization_id
   AND mc.structure_id            = ffsv.id_flex_num
   AND ffsv.id_flex_code          = 'MCAT'
   AND ffsv.application_id        = 401
   AND ffsv.structure_view_name   = 'UMS_ITEM_CATEGORY_NAME'
 GROUP BY rle.requisition_line_id
        , xurh.requisition_header_id
        , rle.uniform_set_line_id
        , ROWNUM
        , rle.inventory_item_id
        , rle.need_by_date
        , rle.remarks
        , rle.reason_code
        , mc.segment1 || '.' || mc.segment2 || '.' || mc.segment3
        , xuel.entitled_qty
        , xuel.category_id
        , xueh.uniform_set_id
        , rle.chargable_qty
        , xuel.renewel_qty
        , xuel.life_expectancy
        ,       --NVL(XX_UMS_ELIGIBLE_QTY_F(XURH.person_id,XUEL.life_expectancy,XUEL.entitled_qty),0) ,
         rle.requested_qty
        , xuel.inventory_organization_id
        , rle.returned_qty
        , rle.damage_qty
        , rle.return_item_id
        , msi.description
        , msi.primary_unit_of_measure
        , xuel.uniform_limited_qty
        , msi.segment1
   ]]></SQLQuery>
