<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="ItemDescForRequisitionVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="xxkq.oracle.apps.xbol.v2.employee.server.ItemDescForRequisitionVORowImpl"
   ComponentClass="xxkq.oracle.apps.xbol.v2.employee.server.ItemDescForRequisitionVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  distinct MC.category_id category_id
       ,MC.segment1||'.'||MC.segment2||'.'||MC.segment3 item_category
       ,MSI.segment1 item_no
         ,MSI.description item_description      
         ,MSI.primary_unit_of_measure uom,
MSI.inventory_item_id item_id    
FROM  MTL_CATEGORIES MC,
      MTL_ITEM_CATEGORIES MIC,
      MTL_SYSTEM_ITEMS MSI
WHERE
      MC.disable_date IS NULL
AND   NVL(MC.enabled_flag,'Y')='Y'
AND     MC.category_id       = MIC.category_id
AND   MIC.inventory_item_id = MSI.inventory_item_id
AND   MIC.organization_id  = MSI.organization_id
--AND   MSI.organization_id  = fnd_profile.value('MFG_ORGANIZATION_ID')
AND   MSI.end_date_active IS NULL
AND   NVL(MSI.enabled_flag,'N') ='Y'
AND   NVL(MSI.attribute11,'NO')='YES'
   ]]></SQLQuery>
